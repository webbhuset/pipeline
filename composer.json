{
    "name": "webbhuset/whaskell",
    "type": "library",
    "license": "proprietary",
    "description": "A library of data-manipulation functions.",
    "require": {
        "php": "^5.4.0 || ^7.0",
        "salsify/json-streaming-parser": "^7.0"
    },
    "require-dev": {
        "webbhuset/test": "*"
    },
    "config": {
        "secure-http": false,
        "minimum-stability": "dev"
    },
    "autoload": {
        "psr-4": {
            "Webbhuset\\Whaskell\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webbhuset\\Whaskell\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages-php.webbhuset.com"
        }
    ]
}
