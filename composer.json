{
    "name": "webbhuset/bifrost",
    "type": "library",
    "license": "proprietary",
    "description": "A bridge between worlds.",
    "require": {
        "php": "^5.4.0 || ^7.0",
        "salsify/json-streaming-parser": "dev-master"
    },
    "require-dev": {
        "webbhuset/test": "dev-master"
    },
    "config": {
        "secure-http": false,
        "minimum-stability": "dev"
    },
    "autoload": {
        "psr-4": {
            "Webbhuset\\Bifrost\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webbhuset\\Bifrost\\Test\\": "tests/core/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.webbhuset.com:libs/test.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:webbhuset/jsonstreamingparser.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.webbhuset.com:libs/bifrost.git"
        }
    ]
}
